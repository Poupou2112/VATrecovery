# Configuration du dashboard
DASHBOARD_USER=admin
DASHBOARD_PASS=

# Identifiants SMTP pour envoi de mails
SMTP_FROM=email@example.com
SMTP_USER=email@example.com
SMTP_PASS=
SMTP_HOST=smtp.example.com
SMTP_PORT=465

# Identifiants IMAP pour réception de mails
IMAP_EMAIL=email@example.com
IMAP_PASSWORD=
IMAP_SERVER=imap.example.com
IMAP_PORT=993

# Authentification Google Cloud (ne pas mettre de JSON inline)
GOOGLE_APPLICATION_CREDENTIALS=/path/to/credentials.json

# Sécurité de l'application
SESSION_SECRET=

# Base de données
# Format recommandé: postgresql://user:password@host:port/dbname
DATABASE_URL=

# ID Client OAuth2
DASHBOARD_CLIENT_ID=
API_TEST_TOKEN=testtoken
